{
  "name": "becompliant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode dev",
    "build:dev_gcp": "tsc && vite build --mode dev_gcp",
    "build:local_docker": "tsc && vite build --mode local_docker",
    "build:prod": "tsc && vite build --mode prod",
    "start": "vite",
    "lint": "eslint  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint --fix  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'",
    "preview": "vite preview",
    "prepare": "cd .. && cd .. && husky frontend/beCompliant/.husky",
    "pre-commit": "lint-staged && tsc --noEmit",
    "test": "vitest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx, js, jsx}": [
      "eslint --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^3.23.0",
    "@azure/msal-react": "^2.0.22",
    "@kvib/react": "^5.1.3",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/react-query": "^5.49.2",
    "@tanstack/react-query-devtools": "^5.49.2",
    "@tanstack/react-table": "^8.19.2",
    "axios": "^1.8.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.6",
    "lucide-react": "^0.487.0",
    "material-symbols": "^0.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router": "^7.5.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "tw-animate-css": "^1.2.5",
    "vitest": "^3.0.9"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.5.3",
    "typescript": "^5.5.3",
    "vite": "^6.2.4"
  }
}
